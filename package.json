{
  "name": "paya",
  "version": "0.1.0",
  "private": true,
  "description": "P2P User-Owned Payment Network",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "db:generate": "pnpm --filter @paya/api db:generate",
    "db:migrate": "pnpm --filter @paya/api db:migrate",
    "db:push": "pnpm --filter @paya/api db:push",
    "db:studio": "pnpm --filter @paya/api db:studio",
    "db:seed": "pnpm --filter @paya/api db:seed",
    "test-data": "pnpm --filter @paya/api test-data",
    "setup:synctera-ops": "pnpm exec tsx scripts/setup-synctera-ops.ts"
  },
  "devDependencies": {
    "tsx": "^4.19.0",
    "turbo": "^2.2.0",
    "typescript": "^5.6.0"
  },
  "packageManager": "pnpm@8.15.5",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
